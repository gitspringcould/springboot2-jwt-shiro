<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zygk.web.domain.local.persistence.DeviceMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.zygk.web.domain.local.entity.Device">
        <id column="id" property="id" />
        <result column="dev_id" property="devId" />
        <result column="dev_name" property="devName" />
        <result column="status" property="status" />
        <result column="create_time" property="createTime" />
        <result column="update_time" property="updateTime" />
        <result column="lon" property="lon" />
        <result column="lati" property="lati" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, dev_id, dev_name, status, create_time, update_time, lon, lati
    </sql>

</mapper>
